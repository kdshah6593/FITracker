<h1><span class="app-name">FI<span class="line">Tracker</span></span></h1>

<% @user.workouts.each do |workout| %>
    <div>
        <h4><%= workout.title%></h4>
        <p><%= workout.workout_type %></p>
        <p><%= workout.created_at %></p>
        <a href="/<%= @user.username %>/workouts/<%= workout.id %>/edit">Edit</a>
        <a href="/<%= @user.username %>/workouts/<%= workout.id %>/delete">Delete</a>
    <div>
<% end %>